#include <stdio.h>
void findMostFrequent(int arr[], int n, int* element, int* frequency) {
    if (n <= 0) {
        *element = 0;
        *frequency = 0;
        return;
    }
    int maxVal = arr[0];
    for (int i = 1; i < n; i++) {
        if (arr[i] > maxVal) {
            maxVal = arr[i];
        }
    }

    int count_arr[maxVal + 1];
    for (int i = 0; i <= maxVal; i++) {
        count_arr[i] = 0;
    }

    for (int i = 0; i < n; i++) {
        count_arr[arr[i]]++;
    }

    int maxFrequency = 0;
    int mostFrequentElement = arr[0];

    for (int i = 0; i <= maxVal; i++) {
        if (count_arr[i] > maxFrequency) {
            maxFrequency = count_arr[i];
            mostFrequentElement = i;
        }
    }

    *element = mostFrequentElement;
    *frequency = maxFrequency;
}
int main() {
    int arr[] = {1, 3, 2, 1, 4, 1, 3, 5, 3, 3};
    int n = sizeof(arr) / sizeof(arr[0]);
    int element;
    int frequency;

    findMostFrequent(arr, n, &element, &frequency);

    printf("Phan tu xuat hien nhieu nhat la: %d\n", element);
    printf("So lan xuat hien: %d\n", frequency);

    return 0;
}
