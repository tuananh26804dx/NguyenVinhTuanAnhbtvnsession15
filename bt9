#include <stdio.h>
#include <limits.h>

int findMinFrequencyElement(int arr[], int size) {
    int minFrequency = INT_MAX;
    int minElement = -1;

    for (int i = 0; i < size; i++) {
        int currentFrequency = 0;
        for (int j = 0; j < size; j++) {
            if (arr[i] == arr[j]) {
                currentFrequency++;
            }
        }
        if (currentFrequency < minFrequency) {
            minFrequency = currentFrequency;
            minElement = arr[i];
        }
    }
    return minElement;
}
int main() {
    int arr[] = {1, 5, 2, 5, 1, 3, 5, 2, 3, 1, 4};
    int size = sizeof(arr) / sizeof(arr[0]);
    
    int result = findMinFrequencyElement(arr, size);
    
    if (result != -1) {
        printf("Phan tu xuat hien it nhat la: %d\n", result);
    } else {
        printf("Mang rong hoac khong the tim thay phan tu.\n");
    }
    
    return 0;
}
